{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "fmt": "prettier --write \"**/*.{ts,tsx,md}\"",
    "turbo": "turbo",
    "dev:common": "turbo run dev --filter timekipr-common",
    "build:desktop": "turbo run tauri:build --filter timekipr-desktop",
